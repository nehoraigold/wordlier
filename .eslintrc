{
    "extends": ["plugin:prettier/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "no-loops"
    ],
    "root": true,
    "rules": {
        "no-console": 1,
        "no-loops/no-loops": 2
    }
}